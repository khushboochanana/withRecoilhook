{
  "presets": [
    "babel-preset-gatsby",
    [
      "@babel/env",
      {
        "modules": "commonjs",
        "shippedProposals": true,
        "targets": {
          "node": "10.13.0"
        }
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    [
      "module-resolver",
      {
        "root": [
          "."
        ]
      }
    ]
  ]
}